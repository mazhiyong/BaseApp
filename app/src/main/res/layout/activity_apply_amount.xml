<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:background="@color/body_bg">

    <include
        android:id="@+id/top_layout"
        layout="@layout/title_leftbut_bar"/>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/scrollView_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/body_bg">


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:layout_marginTop="@dimen/margin_top"
                android:background="@color/divide_line"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp_17"
                    android:text="@string/apply_amount" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/black"
                        android:layout_marginLeft="@dimen/dp_15"
                        android:text="@string/rmb"/>
                    <EditText
                        android:id="@+id/money_edit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:maxLength="14"
                        android:background="@null"
                        android:inputType="numberDecimal"
                        android:textSize="25sp"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:layout_marginLeft="@dimen/item_padding"
                    android:layout_marginRight="@dimen/item_padding"
                    android:layout_marginTop="@dimen/dp_5"
                    android:layout_marginBottom="@dimen/dp_5"
                    android:background="@color/divide_line"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_10"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/tip_icon"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_gravity="center_vertical"
                        android:text="@string/apply_tip"/>

                </LinearLayout>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:layout_marginTop="@dimen/margin_top"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/qixian_lay"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/borrow_limit" />

                    <View
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_1"
                        android:layout_weight="1"/>

                    <TextView
                        android:id="@+id/qixian_value_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:paddingRight="@dimen/dp_10"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:textSize="@dimen/sp_15"

                        />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/pay_type_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"

                        android:text="@string/pay_style" />
                    <View
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_2"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/pay_type_value_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/sp_15"
                        android:focusable="true"
                        android:textColor="@color/black"
                        android:focusableInTouchMode="true"
                        android:paddingRight="@dimen/dp_10"
                        />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/zhongqi_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/pay_zhouqi" />
                    <View
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_1"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/zhouqi_value_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:paddingRight="@dimen/dp_10"
                        android:textSize="@dimen/sp_15"

                        />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:id="@+id/other_zhognqi_line"
                android:background="@color/divide_line"/>

            <androidx.cardview.widget.CardView
                android:id="@+id/other_zhonqi_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"

                        android:text="@string/other_info" />
                    <EditText
                        android:id="@+id/other_zhouqi_eddit"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical|right"
                        android:textSize="@dimen/sp_15"
                        android:layout_weight="1"
                        android:background="@null"
                        android:textColor="@color/black"
                        android:layout_marginLeft="@dimen/dp_30"
                        />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:visibility="gone"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/lilv_type_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:clickable="true"
                android:visibility="gone"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/item_padding"
                    android:paddingRight="@dimen/item_padding"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"

                        android:text="@string/lilv_style" />
                    <View
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_1"
                        android:layout_weight="1"></View>
                    <TextView
                        android:id="@+id/lilv_value_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:textSize="@dimen/sp_15"

                        android:paddingRight="@dimen/dp_10"
                        />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/daikuan_kind_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"

                        android:text="@string/daikuan_kind" />
                    <View
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_2"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/daikuan_kind_value_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:focusable="true"
                        android:textSize="@dimen/sp_15"
                        android:focusableInTouchMode="true"
                        />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/daikuan_use_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/daikuan_use" />
                    <View
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_2"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/daikuan_use_value_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:textSize="@dimen/sp_15"
                        android:paddingRight="@dimen/dp_10"
                        />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>



            <LinearLayout
                android:id="@+id/add_gtpeople_Lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:textSize="@dimen/sp_15"
                    android:text="@string/baoren_info" />


                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>
                <com.github.jdsjlzx.recyclerview.LRecyclerView
                    android:id="@+id/same_people_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:descendantFocusability="blocksDescendants"
                    android:background="@color/white"
                    app:layout_behavior="@string/appbar_scrolling_view_behavior"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/add_same_people_lay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="0dp"
                    app:cardElevation="0dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:paddingLeft="@dimen/item_padding"
                        android:paddingRight="@dimen/item_padding"
                        android:paddingTop="@dimen/item_padding"
                        android:paddingBottom="@dimen/item_padding"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp_15"

                            android:text="@string/add_baoren" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/bulu_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/item_padding"
                    android:paddingRight="@dimen/item_padding"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:textSize="@dimen/sp_15"
                    android:text="@string/has_upload_fujian" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>
                <androidx.cardview.widget.CardView
                    android:id="@+id/has_fujian_lay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="0dp"
                    app:cardElevation="0dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/dp_15"
                        android:paddingRight="@dimen/dp_15"
                        android:paddingTop="@dimen/item_padding"
                        android:paddingBottom="@dimen/item_padding"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/has_upload_tv2"

                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp_15"

                            android:text="@string/has_upload_fujian" />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="@dimen/dp_2"
                            android:layout_weight="1"/>
                        <TextView
                            android:id="@+id/add_file_tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/sp_15"
                            android:textColor="@color/black"
                            android:text="@string/add_fujian"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="@dimen/dp_2"
                            android:layout_weight="1"/>
                        <TextView
                            android:id="@+id/file_num_tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/sp_15"
                            android:textColor="@color/black"
                            android:layout_marginRight="@dimen/dp_10"
                            android:text="0个" />
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/android_list_idex"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <View
                    android:id="@+id/bulu_divide_view"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:visibility="gone"
                    android:background="@color/divide_line"/>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/add_fujian_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:orientation="vertical"
                >

                <TextView
                    android:id="@+id/add_fujian_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:textSize="@dimen/sp_15"
                    android:text="@string/upload_fuji" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/fujian_lay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="0dp"
                    app:cardElevation="0dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/dp_15"
                        android:paddingRight="@dimen/dp_15"
                        android:paddingTop="@dimen/item_padding"
                        android:paddingBottom="@dimen/item_padding"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/has_upload_tv"

                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp_15"

                            android:text="@string/has_upload_fujian" />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="@dimen/dp_2"
                            android:layout_weight="1"/>
                        <TextView
                            android:id="@+id/add_file_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/sp_15"
                            android:textColor="@color/black"
                            android:text="@string/add_fujian"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="@dimen/dp_2"
                            android:layout_weight="1"/>
                        <TextView
                            android:id="@+id/file_num_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/sp_15"
                            android:textColor="@color/black"
                            android:layout_marginRight="@dimen/dp_10"
                            android:text="0个" />
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/android_list_idex"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/xieyi_lay"
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <CheckBox
                    android:id="@+id/cb_xieyi"
                    android:layout_width="@dimen/dp_17"
                    android:checked="true"
                    android:layout_height="@dimen/dp_17"
                    android:layout_marginTop="@dimen/dp_2"
