<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:background="@color/body_bg"
    >

    <include
        android:id="@+id/top_layout"
        layout="@layout/title_leftbut_bar"/>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/scrollView_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/body_bg">



            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left|center_vertical"
                android:textColor="@color/black_middle"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_15"
                android:paddingTop="@dimen/item_padding"
                android:paddingBottom="@dimen/item_padding"
                android:textSize="@dimen/sp_14"
                android:text="@string/hetong_info" />
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_15"
                android:paddingTop="@dimen/item_padding"
                android:paddingBottom="@dimen/item_padding"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp_15"
                    android:text="@string/hetong_code" />
                <EditText
                    android:id="@+id/hetong_code_edit"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:textSize="@dimen/sp_15"
                    android:layout_weight="1"
                    android:background="@null"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/dp_30"
                    android:hint="@string/hetong_code_hint"
                    />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>

            <androidx.cardview.widget.CardView
                android:id="@+id/gmf_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/pay_people" />


                    <TextView
                        android:id="@+id/gmf_value_tv"
                        android:layout_width="287dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_30"
                        android:layout_weight="1"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:gravity="right"
                        android:hint="@string/please_choose"
                        android:paddingRight="@dimen/item_padding"

                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        />

                    <ImageView
                        android:id="@+id/qixian_arrow_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/qd_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/qian_date" />


                    <TextView
                        android:id="@+id/qd_date_value_tv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/black"
                        android:paddingRight="@dimen/item_padding"
                        android:layout_marginLeft="@dimen/dp_30"
                        android:gravity="right"
                        android:hint="@string/qian_date_hint"
                        android:textSize="@dimen/sp_15"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_15"
                android:paddingTop="@dimen/item_padding"
                android:paddingBottom="@dimen/item_padding"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="left|center_vertical"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp_15"
                    android:hint="@string/hetong_code"
                    android:text="@string/jine" />
                <EditText
                    android:id="@+id/money_edit"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical|right"
                    android:textSize="@dimen/sp_15"
                    android:layout_weight="1"
                    android:background="@null"
                    android:inputType="numberDecimal"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/dp_30"
                    android:paddingRight="@dimen/item_padding"
                    android:hint="@string/jine_hint"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp_15"
                    android:text="元" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/js_type_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/js_type" />


                    <TextView
                        android:id="@+id/js_type_value_tv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/black"
                        android:paddingRight="@dimen/item_padding"
                        android:layout_marginLeft="@dimen/dp_30"
                        android:gravity="right"
                        android:hint="@string/js_type_hint"
                        android:textSize="@dimen/sp_15"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>


            <View
                android:id="@+id/jiesuan_date_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/jiesuan_date_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/jiesuan_date" />


                    <TextView
                        android:id="@+id/jiesuan_date_value"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/black"
                        android:paddingRight="@dimen/item_padding"
                        android:layout_marginLeft="@dimen/dp_30"
                        android:gravity="right"
                        android:hint="@string/jiesuan_date_hint"
                        android:textSize="@dimen/sp_15"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:id="@+id/zhouqiJs_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/js_zhouqi" />
                    <EditText
                        android:id="@+id/js_zhouqi_edit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical|right"
                        android:textSize="@dimen/sp_15"
                        android:layout_weight="1"
                        android:background="@null"
                        android:textColor="@color/black"
                        android:layout_marginLeft="@dimen/dp_15"
                        android:paddingRight="@dimen/item_padding"
                        android:hint="@string/js_zhouqi_hint"
                        />
                    <LinearLayout
                        android:id="@+id/danwei_lay"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/danwei_value_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp_15"
                            android:layout_marginRight="@dimen/dp_5"
                            android:text="月" />
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/android_list_idex"/>

                    </LinearLayout>

                </LinearLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="0dp"
                    app:cardElevation="0dp"
                    android:foreground="?android:attr/selectableItemBackground"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/dp_15"
                        android:paddingRight="@dimen/dp_15"
                        android:paddingTop="@dimen/item_padding"
                        android:paddingBottom="@dimen/item_padding"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp_15"
                            android:text="@string/js_date" />



                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.8"
                            android:gravity="center"
                            >

                            <LinearLayout
                                android:id="@+id/wu_lay"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                >
                                <CheckBox
                                    android:id="@+id/wu_checkbox"
                                    android:layout_width="@dimen/dp_18"
                                    android:layout_height="@dimen/dp_18"
                                    android:button="@null"
                                    android:background="@drawable/cart_select_selector"
                                    android:layout_marginRight="@dimen/dp_5"
                                    />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/sp_15"
                                    android:text="无"/>
                            </LinearLayout>

                            <ImageView
                                android:id="@+id/tip_wu_view"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingLeft="@dimen/dp_1"
                                android:paddingRight="@dimen/dp_1"
                                android:src="@drawable/tip"/>


                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center|right"
                            >
                            <LinearLayout
                                android:id="@+id/myue_lay"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                >
                                <CheckBox
                                    android:id="@+id/myue_checkbox"
                                    android:layout_width="@dimen/dp_18"
                                    android:layout_height="@dimen/dp_18"
                                    android:button="@null"
                                    android:background="@drawable/cart_select_selector"
                                    android:layout_marginRight="@dimen/dp_5"
                                    />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/sp_15"
                                    android:text="每月"/>
                            </LinearLayout>

                            <EditText
                                android:maxLength="2"
                                android:id="@+id/myue_edit"
                                android:layout_width="@dimen/dp_30"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/sp_15"
                                android:background="@drawable/edit_background_corners"
                                android:gravity="center"
                                android:layout_marginLeft="@dimen/dp_5"
                                android:layout_marginRight="@dimen/dp_5"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="@dimen/sp_15"
                                android:text="日"/>
                            <ImageView
                                android:id="@+id/tip_ri_view"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingLeft="@dimen/dp_1"
                                android:paddingRight="@dimen/dp_1"
                                android:layout_marginRight="@dimen/dp_5"
                                android:src="@drawable/tip"/>
                        </LinearLayout>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:visibility="gone"
                            android:src="@drawable/android_list_idex"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_15"
                android:paddingTop="@dimen/item_padding"
                android:paddingBottom="@dimen/item_padding"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp_15"
                    android:text="@string/trade_des"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_5"
                    android:background="@color/white" >

                    <EditText
                        android:id="@+id/zy_edit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/hint_zhaiyao"
                        android:textSize="@dimen/sp_15"
                        android:layout_marginRight="@dimen/dp_5"
                        android:background="@null"
                        android:gravity="left|top"
                        android:minLines="3"
                        />

                    <TextView
                        android:id="@+id/count_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/business_des_edit"
                        android:layout_alignRight="@+id/business_des_edit"
                        android:gravity="right"
                        android:padding="@dimen/item_padding"
                        android:textColor="@color/divide_line"
                        android:textSize="@dimen/very_small" />
                </RelativeLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:background="@color/divide_line"/>


            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:layout_marginTop="@dimen/item_padding"
                android:visibility="gone"
                android:background="@color/divide_line"/>

            <androidx.cardview.widget.CardView
                android:id="@+id/cjr_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="0dp"
                app:cardElevation="0dp"
                android:visibility="gone"
                android:foreground="?android:attr/selectableItemBackground"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/item_padding"
                    android:paddingRight="@dimen/item_padding"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_15"
                        android:text="@string/loaner" />


                    <TextView
                        android:id="@+id/cjr_value_tv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/black"
                        android:paddingRight="@dimen/item_padding"
                        android:layout_marginLeft="@dimen/dp_30"
                        android:gravity="right"
                        android:hint="@string/cjr_hint"
                        android:textSize="@dimen/sp_15"

                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/android_list_idex"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/divide_hight"
                android:visibility="gone"
                android:background="@color/divide_line"/>

            <LinearLayout
                android:id="@+id/fujian_content_lay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:paddingTop="@dimen/item_padding"
                    android:paddingBottom="@dimen/item_padding"
                    android:textSize="@dimen/sp_15"
                    android:text="@string/upload_fuji" />


                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>
                <androidx.cardview.widget.CardView
                    android:id="@+id/fujian_lay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="0dp"
                    app:cardElevation="0dp"
                    android:clickable="true"
                    android:foreground="?android:attr/selectableItemBackground"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/dp_15"
                        android:paddingRight="@dimen/dp_15"
                        android:paddingTop="@dimen/item_padding"
                        android:paddingBottom="@dimen/item_padding"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/has_upload_tv"

                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp_15"
                            android:text="@string/has_upload_fujian" />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="@dimen/dp_2"
                            android:layout_weight="1"/>
                        <TextView
                            android:id="@+id/add_file_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/sp_15"
                            android:textColor="@color/black"
                            android:text="@string/add_fujian"
                            />
                        <View
                            android:layout_width="0dp"
                            android:layout_height="@dimen/dp_2"
                            android:layout_weight="1"/>
                        <TextView
                            android:id="@+id/file_num_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="@dimen/sp_15"
                            android:textColor="@color/black"
                            android:layout_marginRight="@dimen/item_padding"
                            android:text="0个" />
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/android_list_idex"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/divide_hight"
                    android:background="@color/divide_line"/>

            </LinearLayout>



            <Button
                android:id="@+id/but_submit"
                android:layout_width="match_parent"
                android:layout_height="@dimen/but_height"
                android:text="@string/but_next"
                android:background="@drawable/btn_next"
